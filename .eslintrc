{
    "parser": "babel-eslint",
    "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
        }
    },
    "extends": "airbnb",
    "env": {
    "node": true,
    "browser": true,
    "es6": true
    },
    "rules": {
    "comma-dangle": ["error", "never"],
    "max-len": ["off"],
    "no-unused-vars": ["error", {"vars": "local", "args": "none"}],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/extensions": ['error', 'always', {ignorePackages: true} ]
    }
}
